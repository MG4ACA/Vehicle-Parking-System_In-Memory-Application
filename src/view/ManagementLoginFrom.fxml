      <ImageView fitHeight="83.0" fitWidth="81.0" layoutX="183.0" layoutY="22.0" opacity="0.64" pickOnBounds="true" preserveRatio="true" style="-fx-opacity: 50%;">
         <image>
            <Image>
               <url>@../assets/User-Icon%20.png</url>
            </Image>
         </image>
         <effect>
            <Blend mode="BLUE" />
         </effect>
      </ImageView>
      <Label alignment="CENTER_RIGHT" layoutX="36.0" layoutY="136.0" prefHeight="30.0" prefWidth="103.0" text="User Name">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label alignment="CENTER_RIGHT" layoutX="36.0" layoutY="181.0" prefHeight="30.0" prefWidth="103.0" text="Password">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="txtUserName" layoutX="161.0" layoutY="136.0" prefHeight="30.0" prefWidth="248.0" style="-fx-background-color: #A2BCC6;" />
      <TextField fx:id="txtPassword" layoutX="161.0" layoutY="183.0" prefHeight="30.0" prefWidth="248.0" style="-fx-background-color: #A2BCC6;" />
